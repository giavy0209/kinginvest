<%
    var sub_title = getData('sub_title')
    var show_sub_title = getData('show_sub_title')
    var title = getData('title')
    var show_title = getData('show_title')
    var background_image = getData('background_image')
    var block = getData('block')
    
%>
<div data-id="<%=data.id%>" id="block5" class="block" style="background-image: url(<%=background_image%>);">
    <div class="kdg-container">
        <div class="block-title2 text-c">
            <%if(show_sub_title){%>
                <p><%=sub_title%></p>    
            <%}%>
            <%if(show_title){%>
                <h2><%=title%></h2>    
            <%}%>
        </div>
        <div class="maincontent">
            <div class="kdg-row">
                <%block.forEach(el => {%>
                    <div class="kdg-col-6 va-t">
                        <div class="content">
                            <div class="image">
                                <img src="<%=el.img_url%>" alt="">
                            </div>
                            <h4 class="title"><%=el.title%></h4>
                            <div class="des"><%-el.description%></div>
                        </div>
                    </div>
                <%})%>
            </div>
        </div>
    </div>
</div>

